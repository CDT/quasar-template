{
  "version": "0.2.0",
  "configurations": [    
    {
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "nodemon",
      "name": "back",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}\\back\\server.js"
    },
    {
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "nodemon",
      "name": "back tests",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}\\back\\tests\\test.js"
    },
    {
      "name": "front(edge)",
      "request": "launch",
      "type": "msedge",
      "url": "http://localhost:9000",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "front(chrome)",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:9000",
      "webRoot": "${workspaceFolder}"
    }
  ]
}